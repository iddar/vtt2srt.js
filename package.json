    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "n-readlines": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.12",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  }
