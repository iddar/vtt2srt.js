{
  "name": "vtt2srt",
  "version": "1.0.0",
  "description": "comver sub from vvt to srt",
  "main": "app.js",
  "scripts": {
    "start": "node -r ts-node/register ./src/app.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "author": "iddar <iddar@dbug.mx>",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
